{%  set classes = [
    'general-purpose-component--default-banner'
] %}


<div {{ attributes.addClass(classes) }}>
	<div class="banner-content">
		{% if content.field_title %}
			{{ content.field_title }}
		{% endif %}
	</div>
	<div class="banner-image">
		{% if content.field_image %}
			{{content.field_image}}
		{% endif %}
	</div>
</div>